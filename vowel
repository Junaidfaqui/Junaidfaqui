// Online C compiler to run C program online
#include <stdio.h>

void main() {
    char cha;
    printf("enter the char ");
    scanf("%c",&cha);
if(cha=='a'||cha=='e'||cha=='i'||cha=='o'||cha=='u'||cha=='A'||cha=='E'||cha=='I'||cha=='O'||cha=='U')
    {
        printf("it is a vowel");
    }
    else
    {
        printf("it is not vowel");
    }
}
